        </LinearLayout>

        <TextView
            android:id="@+id/TV_vm_vm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="70dp"
            android:text="Visi dan Misi Perusahaan"
            android:textSize="22dp"
            android:textStyle="bold" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/TV_vm_vm"
            android:layout_marginTop="10dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/textView7_vm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="14dp"
                    android:background="@color/trans2"
                    android:layout_centerHorizontal="true"
                    android:text="Visi : "
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <android.support.v7.widget.CardView
                    android:id="@+id/CV_vm_visi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView7_vm"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginRight="4dp">
                    <TextView
                        android:id="@+id/TV_vm_visi"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/trans2"
                        android:textSize="14dp"
                        android:textColor="@color/black"
                        android:elevation="100dp"
                        android:text="- isi Visi " />

                </android.support.v7.widget.CardView>


                <TextView
                    android:id="@+id/textView10_vm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/CV_vm_visi"
                    android:layout_alignStart="@+id/textView7_vm"
                    android:layout_marginStart="1dp"
                    android:layout_marginTop="12dp"
                    android:text="Misi : "
                    android:textSize="18sp"
                    android:background="@color/trans2"
                    android:textStyle="bold" />

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textView10_vm"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginRight="4dp">
                    <TextView
                        android:id="@+id/TV_vm_misi"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/trans2"
                        android:elevation="100dp"
                        android:textColor="@color/black"
                        android:text="- isi misi" />
                </android.support.v7.widget.CardView>

            </RelativeLayout>

        </ScrollView>
    <android.support.design.widget.FloatingActionButton
